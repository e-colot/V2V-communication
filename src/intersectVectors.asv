function coord = intersectVectors(v1, v2)

    d1 = (v1(2,:) - v1(1,:))/norm(v1(2,:) - v1(1,:));
    d2 = (v2(2,:) - v2(1,:))/norm(v2(2,:) - v2(1,:));
    n1 = [d1(2), -d1(1)];

    perpDRatio = dot(v1(1,:) - v2(1,:), n1)/dot(d2, n1);
    if ((abs(perpDRatio) > 1) ||(norm()))
        coord = [NaN, NaN];
    else
        coord = v2(1,:) + perpDRatio*d2;
    end

end

